{
  "name": "mern-boilerplate",
  "version": "1.0.0",
  "type": "module",
  "description": "A full stack application boilerplate using MERN stack with redux and jwt for authentication",
  "main": "server",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "nodemon backend/server",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "import": "node backend/seeder",
    "destroy": "node backend/seeder -d",
    "build":"npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aditya25022001/mern-boilerplate.git"
  },
  "keywords": [
    "MERN",
    "boilerplate",
    "starter",
    "redux",
    "jwt",
    "authentication",
    "full-stack"
  ],
  "author": "Aditya Uday Ubale",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aditya25022001/mern-boilerplate/issues"
  },
  "homepage": "https://github.com/aditya25022001/mern-boilerplate#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.4.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.13",
    "multer": "^1.4.3",
    "nodemailer": "^6.7.1",
    "nodemailer-express-handlebars": "^4.0.0",
    "nodemon": "^2.0.15",
    "ulid": "^2.3.0"
  }
}
